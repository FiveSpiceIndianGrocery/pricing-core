{
  "name": "pricing-core",
  "version": "3.0.0",
  "description": "PricingCore - A high-precision, currency-agnostic pricing engine with BigInt precision, multiple markup strategies, multiple rounding strategies, and ISO 4217 currency support via currency-codes package",
  "main": "src/index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./rounding": "./src/rounding/index.js",
    "./math": "./src/core/math.js",
    "./currency": "./src/currency.js"
  },
  "files": [
    "src/**/*.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node test/quick-check.js",
    "test:watch": "node test/quick-check.js --watch",
    "examples": "node examples.js",
    "cli": "node cli.js",
    "build": "echo 'No build step needed for ES modules'",
    "prepublishOnly": "npm test",
    "lint": "echo 'Add your preferred linter here'",
    "docs": "echo 'Documentation is in README.md'",
    "prepack": "npm run test",
    "postpack": "echo 'Package created successfully'",
    "dev": "npm run test && npm run examples",
    "start": "npm run cli",
    "demo": "npm run examples",
    "version:patch": "./scripts/version.sh patch --auto-commit",
    "version:minor": "./scripts/version.sh minor --auto-commit",
    "version:major": "./scripts/version.sh major --auto-commit",
    "version:dry-run": "./scripts/version.sh patch --dry-run",
    "release:patch": "npm run version:patch",
    "release:minor": "npm run version:minor",
    "release:major": "npm run version:major"
  },
  "keywords": [
    "pricing",
    "calculator",
    "rounding",
    "business-logic",
    "bigint",
    "precision",
    "currency",
    "financial",
    "ecommerce",
    "retail",
    "iso4217",
    "margin",
    "markup",
    "cost-plus",
    "keystone",
    "profit",
    "core",
    "engine",
    "typescript",
    "node",
    "javascript"
  ],
  "author": "Kunal Advani <kunal.advani@example.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FiveSpiceIndianGrocery/pricing-core.git"
  },
  "bugs": {
    "url": "https://github.com/FiveSpiceIndianGrocery/pricing-core/issues"
  },
  "homepage": "https://github.com/FiveSpiceIndianGrocery/pricing-core#readme",
  "dependencies": {
    "currency-codes": "^2.2.0"
  }
}
